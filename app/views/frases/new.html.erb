<%= link_to "logout (#{ current_user.nombre })", logout_path, method: 'delete' %>

<h1>Frases</h1>
<%= form_for(@frase) do |f| %>
    <% if user_frase?  %>
        <p>ya ingresaste la frase, y es <%= estado_frase %></p>
    <% else %>
        <div>
            <%= f.label :frase, "ingrese la frase"%>
            <%= f.text_field :frase %>
            <%= f.submit "enviar la frase" %>
        </div>
    <% end%>
<% end %>

